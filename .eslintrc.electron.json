{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended-type-checked",
    "plugin:@typescript-eslint/stylistic-type-checked",
    "plugin:import/recommended",
    "plugin:import/electron",
    "plugin:import/typescript"
  ],
  "parserOptions": {
    "project": true,
    "tsconfigRootDir": "."
  },
  "parser": "@typescript-eslint/parser",
  "ignorePatterns": [
    "renderer"
  ]
}
